{
    "manifest_version": 2,
    "name": "CourseOff Registration for UMD",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "https://umd.courseoff.com/workspace*"
            ],
            "js": ["jquery-3.4.1.min.js", "content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}
